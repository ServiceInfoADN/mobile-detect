{
    "name": "serviceinfoadn/mobiledetect",
    "version": "4.8.1",
    "type": "library",
    "description": "Mobile_Detect is a lightweight PHP class for detecting mobile devices. It uses the User-Agent string combined with specific HTTP headers to detect the mobile environment.",
    "keywords": ["mobile", "mobile detect", "mobile detector", "php mobile detect", "detect mobile devices"],
    "license": "MIT",
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^v3.35.1",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7",
        "phpbench/phpbench": "^1.2",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Detection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DetectionTests\\": "tests/"
        }
    },
    "archive": {
      "exclude": ["scripts"]
    },
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/ServiceInfoADN/mobile-detect.git"
      }
    ]
}
